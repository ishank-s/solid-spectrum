{
  "name": "solid-spectrum-monorepo",
  "description": "your-repository-desc",
  "version": "0.0.1",
  "license": "MIT",
  "author": "your-author-name",
  "contributors": [],
  "private": true,
  "scripts": {
    "dev": "pnpm -F \"./packages/**\" --parallel --stream -r run dev",
    "build": "turbo run build --filter=./packages/*",
    "test": "turbo run test --filter=./packages/*",
    "typecheck": "turbo run typecheck --filter=./packages/*",
    "build-test": "turbo run build test typecheck --filter=./packages/*",
    "format": "prettier --ignore-path .gitignore -w \"packages/**/*.{js,ts,json,css,tsx,jsx,md}\" \"playgrounds/**/*.{js,ts,json,css,tsx,jsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version && pnpm i",
    "ci:version": "changeset version && pnpm i --no-frozen-lockfile && git add .",
    "release": "pnpm build && changeset publish",
    "update-deps": "pnpm up -Lri",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@solid-aria/types": "^0.1.4",
    "@solid-primitives/platform": "^0.1.1",
    "@solid-primitives/props": "^3.1.10",
    "@solid-primitives/utils": "^6.2.3",
    "solid-js": "^1.8.15"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-controls": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/node": "^18.19.21",
    "concurrently": "^7.6.0",
    "jsdom": "^21.1.2",
    "prettier": "2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.17",
    "storybook-dark-mode": "^3.0.3",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "tsup": "^6.7.0",
    "tsup-preset-solid": "^0.1.8",
    "turbo": "^1.12.4",
    "typescript": "5.1.1-rc",
    "vite": "^4.5.2",
    "vite-plugin-solid": "^2.10.1",
    "vitest": "^0.27.3"
  },
  "packageManager": "pnpm@7.22.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "20.2.0"
  }
}
