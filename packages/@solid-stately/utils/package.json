{
  "name": "@solid-stately/utils",
  "version": "0.0.1",
  "description": "Spectrum UI components in Solid",
  "license": "Apache-2.0",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/import.mjs"
  },
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "dev": "tsup --watch",
