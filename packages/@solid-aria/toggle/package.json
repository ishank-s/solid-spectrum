{
  "name": "@solid-aria/toggle",
  "version": "0.0.1",
  "description": "Toggle Spectrum UI components in Solid",
  "license": "Apache-2.0",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/import.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test:client --mode ssr",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "dependencies": {
    "@react-types/checkbox": "^3.7.1",
    "@react-types/shared": "^3.22.1",
    "@solid-aria/interactions": "workspace:^",
    "@solid-aria/utils": "workspace:^",
    "@solid-stately/toggle": "workspace:^",
    "@solid-stately/utils": "workspace:^",
    "solid-js": "^1.8.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
